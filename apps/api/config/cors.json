{
  "cors": {
    "allowedHeaders": [
      "Authorization",
      "Content-Type",
      "accept-language",
      "x-trpc-source",
      "x-user-locale",
      "x-user-timezone",
      "x-user-country"
    ],
    "allowMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"],
    "exposeHeaders": ["Content-Length"],
    "maxAge": 86400,
    "origin": "%env(BASANGO_API_ALLOWED_ORIGINS)%"
  }
}
