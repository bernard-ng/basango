{
  "compilerOptions": {
    "paths": {
      "#api/*": ["../api/src/*"],
      "#dashboard/*": ["./src/*"],
      "#db/*": ["../../packages/db/src/*"],
      "#domain/*": ["../../packages/domain/src/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": ["node_modules"],
  "extends": "@basango/tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "next.config.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"]
}
